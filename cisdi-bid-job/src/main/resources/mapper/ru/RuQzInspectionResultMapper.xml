<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pms.bid.job.mapper.ru.RuQzInspectionResultMapper">


    <select id="selectOneByZh" resultType="com.pms.bid.job.domain.qingZhu.RuQzInspectionResult">
        SELECT  id,`name`,remark
        from
            ru_qz_inspection_result  T
        where  T.`NAME`->>'$.ZH_CN'=#{zhName}
    </select>

</mapper>